From 165517eea83629e61bf6641985ca5f415fbf06be Mon Sep 17 00:00:00 2001
From: =?UTF-8?q?Urs=20F=C3=A4ssler?= <urs@bitzgi.ch>
Date: Sat, 24 Oct 2015 14:48:39 +0200
Subject: [PATCH 1/3] device tree for beagle bone green

---
 arch/arm/boot/dts/am335x-bonegreen.dts  |  3 +++
 arch/arm/boot/dts/am335x-bonegreen.dtsi | 34 +++++++++++++++++++++++++++++++++
 2 files changed, 37 insertions(+)
 create mode 100644 arch/arm/boot/dts/am335x-bonegreen.dts
 create mode 100644 arch/arm/boot/dts/am335x-bonegreen.dtsi

diff --git a/arch/arm/boot/dts/am335x-bonegreen.dts b/arch/arm/boot/dts/am335x-bonegreen.dts
new file mode 100644
index 0000000..04b4e14
--- /dev/null
+++ b/arch/arm/boot/dts/am335x-bonegreen.dts
@@ -0,0 +1,3 @@
+/dts-v1/;
+
+#include "am335x-bonegreen.dtsi"
diff --git a/arch/arm/boot/dts/am335x-bonegreen.dtsi b/arch/arm/boot/dts/am335x-bonegreen.dtsi
new file mode 100644
index 0000000..e0b7c11
--- /dev/null
+++ b/arch/arm/boot/dts/am335x-bonegreen.dtsi
@@ -0,0 +1,34 @@
+/*
+ * From am335x-boneblack:
+ * Copyright (C) 2012 Texas Instruments Incorporated - http://www.ti.com/
+ */
+
+#include "am33xx.dtsi"
+#include "am335x-bone-common.dtsi"
+
+/ {
+  model = "TI AM335x BeagleBone Green";
+        compatible = "ti,am335x-bone-green", "ti,am335x-bone", "ti,am33xx";
+};
+
+&ldo3_reg {
+        regulator-min-microvolt = <1800000>;
+        regulator-max-microvolt = <1800000>;
+        regulator-always-on;
+};
+
+&mmc1 {
+        vmmc-supply = <&vmmcsd_fixed>;
+};
+
+&mmc2 {
+        vmmc-supply = <&vmmcsd_fixed>;
+        pinctrl-names = "default";
+        pinctrl-0 = <&emmc_pins>;
+        bus-width = <8>;
+        status = "okay";
+};
+
+&rtc {
+        system-power-controller;
+};
-- 
